 <header class="p-3 bg-dark text-white">
    <div class="container">
      <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
        <%= link_to 'QUIZ APP', root_path, class: 'nav-link px-2 text-secondary'%>
        <% if user_signed_in? %>
            <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
              <li><%= link_to 'Home', root_path, class: 'nav-link px-2 text-secondary'%></li>
              <li><%= link_to "New quiz test", new_quiz_assessment_path, class: 'nav-link px-2 text-secondary' %> </li>
            </ul>
        <form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3">
          <input type="search" class="form-control form-control-dark" placeholder="Search..." aria-label="Search">
        </form>

         <div class="text-end">
          <%= link_to "Sign out", destroy_user_session_path, class: "btn btn-warning" , method: :delete%>
        </div>
      <% else %>
       
            <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
              <li><%= link_to 'Home', root_path, class: 'nav-link px-2 text-secondary'%></li>        
            </ul>
        <div class="text-end">
          <%= link_to "Sign in", user_session_path,  class: "btn btn-outline-light me-2" %>
          <%= link_to "Sign up", new_user_registration_path, class: "btn btn-warning" %>

        
          
        </div>
      <% end %>
      </div>
    </div>
  </header>